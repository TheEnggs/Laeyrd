{
  "name": "Laeyrd",
  "version": "1.0.1",
  "description": "Laeyrd is a theme and settings customizer for VSCode with sync functionality.",
  "publisher": "TheEnggs",
  "readme": "README.extension.md",
  "icon": "dist/media/icons/laeyrd_logo.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theenggs/laeyrd"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "main": "dist/extension.js",
  "type": "commonjs",
  "activationEvents": [
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "keywords": [
    "theme",
    "editor",
    "vscode",
    "laeyrd"
  ],
  "contributes": {
    "commands": [
      {
        "command": "laeyrd.open",
        "title": "Laeyrd | TheEnggs",
        "icon": "dist/media/icons/laeyrd_ae_dark.svg",
        "category": "Customization"
      }
    ],
    "themes": []
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --parallel dev:*",
    "dev:js": "tsc --watch",
    "dev:webview": "vite --config packages/webview-ui/vite.config.ts",
    "build": "npm run build:extension  && npm run build:webview && npm run copy:media",
    "build:extension": "NODE_ENV=production node packages/extension/esbuild.config.mjs",
    "build:webview": "vite build --config packages/webview-ui/vite.config.ts",
    "typecheck": "tsc --noEmit -p tsconfig.base.json",
    "copy:media": "mkdir -p dist/media/icons && cp -r packages/media/icons/* dist/media/icons/",
    "prepublishOnly": "npm run build",
    "package": "rm -rf dist && npm run build && mkdir -p dist/release && vsce package --no-yarn --out dist/release/$(node -p \"require('./package.json').name\")-$(node -p \"require('./package.json').version\").vsix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --concurrency auto",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --concurrency auto"
  },
  "author": "TheEnggs",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.9.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.103.0",
    "@types/vscode-webview": "1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.12",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "tsc-alias": "^1.8.16",
    "tsx": "latest",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "diff": "^8.0.2",
    "embla-carousel-react": "8.5.1",
    "framer-motion": "^12.23.12",
    "jsonc-parser": "^3.3.1",
    "lucide-react": "^0.536.0",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "sonner": "latest",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.6",
    "uuid": "^13.0.0",
    "vscode-extension-telemetry": "^0.4.5",
    "zod": "^3.24.1"
  }
}
