{
  "name": "laeyrd",
  "version": "0.0.1",
  "description": "",
  "main": "dist/extension/extension.js",
  "publisher": "theenggs",
  "icon": "dist/media/logo.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theenggs/laeyrd"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --parallel dev:*",
    "dev:js": "tsc --watch",
    "dev:webview": "vite",
    "build:ts": "tsc -b && tsc-alias",
    "build:webview": "vite build",
    "build": "npm-run-all build:ts build:webview copy:themes copy:snippets copy:media",
    "copy:themes": "cp -r src/themes dist/",
    "copy:media": "mkdir -p dist/media && cp -r src/media/* dist/media/",
    "copy:snippets": "mkdir -p dist/extension && cp -r src/extension/snippets dist/extension/",
    "prepublishOnly": "npm run build",
    "package": "mkdir -p dist/release && vsce package --out dist/release/$(node -p \"require('./package.json').name\")-$(node -p \"require('./package.json').version\").vsix"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "laeyrd.deviceMachineId": {
          "type": "string",
          "default": "",
          "description": "Unique machine ID for device authentication"
        }
      }
    },
    "commands": [
      {
        "command": "laeyrd.open",
        "title": "Laeyrd | TheEnggs",
        "icon": "dist/media/logo.png",
        "category": "Customization"
      }
    ],
    "themes": [
      {
        "label": "Live Preview - Laeyrd",
        "uiTheme": "vs-dark",
        "path": "/themes/livepreview-laeyrd.json"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.80.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "vite": "latest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "4.1.0",
    "diff": "^8.0.2",
